                           ] .


###  http://www.semanticweb.org/mah/ontologies/2022/9/untitled-ontology-8#product
:product rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSupplier ;
                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :supplier
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasName ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :product
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasPrice ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass :product
                             ] ;
         rdfs:comment "class representing company products" .


###  http://www.semanticweb.org/mah/ontologies/2022/9/untitled-ontology-8#supplier
:supplier rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty :isLocatedIn ;
                                                owl:someValuesFrom :belgianCity
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :isLocatedIn ;
                                                owl:someValuesFrom :dutchCity
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty :isLocatedIn ;
                                                owl:someValuesFrom :germanCity
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasName ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :supplier
                              ] ;
          rdfs:comment "class representing product suppliers" .


